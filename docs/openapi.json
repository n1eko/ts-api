      "get": {
        "tags": [
          "Logs"
        ],
        "summary": "Get connection logs",
        "description": "Get connection logs",
        "operationId": "getLogs",
        "responses": {
          "200": {
            "description": "successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
